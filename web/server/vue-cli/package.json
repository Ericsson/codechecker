{
  "name": "codechecker",
  "version": "1.0.0",
  "description": "CodeChecker viewer.",
  "author": "Márton Csordás",
  "private": true,
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "server:codechecker": "CodeChecker server --port 8002 -w e2e/__codechecker/workspace > e2e/__codechecker/server.log",
    "server:dev": "cross-env NODE_ENV=development webpack server --hot --progress --config config/webpack.dev.js",
    "server:prod": "http-server dist -c-1 --cors",
    "server": "npm run server:dev",
    "start": "npm run server",
    "test:clean": "npm run test:e2e.clean",
    "test:e2e": "npm run test:e2e.firefox && npm run test:e2e.chrome",
    "test:e2e.clean": "rimraf e2e/output e2e/screenshots e2e/__codechecker/workspace e2e/__codechecker/reports e2e/__codechecker/compilation_database",
    "test:e2e.chrome": "server-test test:e2e.server http-get://localhost:8002/ready 'npm run test:e2e.init-db && npm run test:nightwatch -- -e selenium.chrome'",
    "test:e2e.firefox": "server-test test:e2e.server http-get://localhost:8002/ready 'npm run test:e2e.init-db && npm run test:nightwatch -- -e selenium.firefox'",
    "test:e2e.init-db": "node e2e/init.db.js",
    "test:e2e.init-reports": "node e2e/init.reports.js",
    "test:e2e.init-workspace": "node e2e/init.workspace.js",
    "test:e2e.server": "npm run test:e2e.init-reports && rimraf e2e/__codechecker/workspace && npm run test:e2e.init-workspace && npm run server:codechecker",
    "test:lint": "eslint --ext .js,.vue src",
    "test:nightwatch": "nightwatch",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "codechecker-api": "file:../../api/js/codechecker-api-node/dist/codechecker-api-6.53.0.tgz",
    "codemirror": "^5.65.0",
    "date-fns": "^2.28.0",
    "js-cookie": "^3.0.1",
    "jsplumb": "^2.15.6",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "splitpanes": "^2.3.8",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@types/jest": "^27.4.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^28.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.3",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^10.2.0",
    "core-js": "^3.20.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.7.0",
    "eslint-plugin-vue": "^8.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "geckodriver": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "jest": "^28.1.1",
    "nightwatch": "^3.1.3",
    "raw-loader": "^4.0.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass": "~1.32.13",
    "sass-loader": "^12.4.0",
    "selenium-server": "^3.141.59",
    "start-server-and-test": "^1.14.0",
    "util": "^0.12.4",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "@cc/report-server-types": "codechecker-api/lib/report_server_types.js",
      "@cc/prod-types": "codechecker-api/lib/products_types.js"
    },
    "transform": {
      ".*\\.(vue)$": "@vue/vue2-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
