{
  "name": "codechecker",
  "version": "1.0.0",
  "description": "CodeChecker viewer.",
  "author": "Márton Csordás",
  "private": true,
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "server:codechecker": "CodeChecker server -w e2e/__codechecker/workspace > e2e/__codechecker/server.log",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --config config/webpack.dev.js",
    "server:prod": "http-server dist -c-1 --cors",
    "server": "npm run server:dev",
    "start": "npm run server",
    "test:clean": "npm run test:e2e.clean",
    "test:e2e": "npm run test:e2e.firefox && npm run test:e2e.chrome",
    "test:e2e.clean": "rimraf e2e/output e2e/screenshots e2e/__codechecker/workspace e2e/__codechecker/reports e2e/__codechecker/compilation_database",
    "test:e2e.chrome": "server-test test:e2e.server http-get://localhost:8001/ready 'npm run test:e2e.init-db && npm run test:nightwatch -- -e selenium.chrome'",
    "test:e2e.firefox": "server-test test:e2e.server http-get://localhost:8001/ready 'npm run test:e2e.init-db && npm run test:nightwatch -- -e selenium.firefox'",
    "test:e2e.init-db": "node e2e/init.db.js",
    "test:e2e.init-reports": "node e2e/init.reports.js",
    "test:e2e.init-workspace": "node e2e/init.workspace.js",
    "test:e2e.server": "npm run test:e2e.init-reports && rimraf e2e/__codechecker/workspace && npm run test:e2e.init-workspace && npm run server:codechecker",
    "test:lint": "eslint --ext .js,.vue src",
    "test:nightwatch": "nightwatch",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "codechecker-api": "6.36.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "^0.7.0",
    "codemirror": "^5.55.0",
    "date-fns": "^2.14.0",
    "highlight.js": "^10.1.1",
    "jsplumb": "^2.13.3",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "marked": "^1.1.0",
    "splitpanes": "^2.2.1",
    "vue": "^2.6.11",
    "vuetify": "^2.3.3",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.3",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "chromedriver": "^87.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "cross-env": "^7.0.2",
    "deepmerge": "^4.2.2",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "geckodriver": "^1.21.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "jest": "^26.1.0",
    "nightwatch": "^1.5.1",
    "raw-loader": "^4.0.1",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "selenium-server": "^3.141.59",
    "start-server-and-test": "^1.11.0",
    "vue-jest": "^3.0.6",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.5.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">=10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "@cc/report-server-types": "codechecker-api/lib/report_server_types.js"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    }
  }
}
